<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout11"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    android:layout_gravity="center"
    tools:context=".fragment.setting_four.choose_lolcal_work.select_work_curtain">


    <TextView
        android:id="@+id/textView31"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:text="Select Curtain"
        android:textColor="@color/white"
        android:textSize="@dimen/_10font_mdp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_conversion_absoluteHeight="21dp"
        tools:layout_conversion_absoluteWidth="265dp" />


    <Button
        android:id="@+id/sw_chose_curtain"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintHeight_percent="0.12"
        android:layout_marginStart="24dp"


        android:layout_marginTop="8dp"
        android:layout_marginEnd="24dp"
        android:background="@drawable/select_back"

        android:text="select"
        android:textColor="@color/white"
        android:textSize="@dimen/_10font_mdp"
        app:layout_constraintBottom_toTopOf="@+id/textView32"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView31"
        app:layout_constraintVertical_bias="0.5"
        tools:layout_conversion_absoluteHeight="45dp"
        tools:layout_conversion_absoluteWidth="265dp" />

    <TextView
        android:id="@+id/textView32"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Select Functionality"
        android:textColor="@color/white"
        android:textSize="@dimen/_9font_mdp"
        app:layout_constraintBottom_toTopOf="@+id/curtain_func_open"
        app:layout_constraintStart_toStartOf="@+id/sw_chose_curtain"
        app:layout_constraintTop_toBottomOf="@+id/sw_chose_curtain"
        app:layout_constraintVertical_bias="0.5"
        tools:layout_conversion_absoluteHeight="21dp"
        tools:layout_conversion_absoluteWidth="265dp" />

    <Button
        android:id="@+id/curtain_func_open"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="8dp"


        android:background="@drawable/functionality_open_anable_off"
        app:layout_constraintBottom_toTopOf="@+id/curtain_func_close"
        app:layout_constraintEnd_toEndOf="@+id/sw_chose_curtain"
        app:layout_constraintHeight_percent="0.12"
        app:layout_constraintStart_toStartOf="@+id/sw_chose_curtain"
        app:layout_constraintTop_toBottomOf="@+id/textView32"
        app:layout_constraintVertical_bias="0.5"
        tools:layout_conversion_absoluteHeight="45dp"
        tools:layout_conversion_absoluteWidth="265dp" />

    <Button
        android:id="@+id/curtain_func_close"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="8dp"


        android:background="@drawable/functionality_close_anable_off"
        app:layout_constraintBottom_toTopOf="@+id/curtain_func_halfo"
        app:layout_constraintEnd_toEndOf="@+id/curtain_func_open"
        app:layout_constraintHeight_percent="0.12"
        app:layout_constraintStart_toStartOf="@+id/curtain_func_open"
        app:layout_constraintTop_toBottomOf="@+id/curtain_func_open"
        app:layout_constraintVertical_bias="0.5"
        tools:layout_conversion_absoluteHeight="45dp"
        tools:layout_conversion_absoluteWidth="265dp" />

    <Button
        android:id="@+id/curtain_func_halfo"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="8dp"


        android:background="@drawable/functionality_halfopen_anable_off"
        app:layout_constraintBottom_toTopOf="@+id/sw_chose_curtain_cancel"
        app:layout_constraintEnd_toEndOf="@+id/curtain_func_close"
        app:layout_constraintHeight_percent="0.13"
        app:layout_constraintStart_toStartOf="@+id/curtain_func_close"
        app:layout_constraintTop_toBottomOf="@+id/curtain_func_close"
        app:layout_constraintVertical_bias="0.5"
        tools:layout_conversion_absoluteHeight="45dp"
        tools:layout_conversion_absoluteWidth="265dp" />


    <Button
        android:id="@+id/sw_chose_curtain_cancel"

        android:layout_width="@dimen/_40mdp"
        android:layout_height="@dimen/_15mdp"
        android:background="@drawable/back_btn_color"
        android:text="cncel"
        android:textColor="@color/white"
        android:textSize="@dimen/_7font_mdp"
        android:layout_marginTop="4dp"



        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/curtain_func_halfo"
        app:layout_constraintHeight_percent="0.12"
        app:layout_constraintStart_toStartOf="@+id/curtain_func_halfo"
        app:layout_constraintTop_toBottomOf="@+id/curtain_func_halfo"
        app:layout_constraintVertical_bias="0.5"
        app:layout_constraintWidth_percent="0.35"
        tools:layout_conversion_absoluteHeight="45dp"
        tools:layout_conversion_absoluteWidth="113dp" />

    <Button
        android:id="@+id/sw_chose_curtain_ok"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/ok"

        android:visibility="invisible"
        tools:ignore="MissingConstraints"
        tools:layout_conversion_absoluteHeight="45dp"
        tools:layout_conversion_absoluteWidth="112dp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="306dp" />


</androidx.constraintlayout.widget.ConstraintLayout>