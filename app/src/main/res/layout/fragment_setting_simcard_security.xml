<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragment.setting_four.setting_simcard_security">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/linearLayout19"
        android:layout_width="match_parent"
        android:layout_height="match_parent"

        android:layout_weight="1">


        <TextView

            android:id="@+id/textView65"
            android:layout_width="wrap_content"

            android:layout_height="wrap_content"

            android:layout_marginLeft="@dimen/_8mdp"
            android:layout_marginTop="16dp"
            android:text="Security"
            android:textColor="@color/white"
            android:textSize="@dimen/_10font_mdp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_5mdp"

            android:text="User"
            android:textColor="@color/white"
            android:textSize="@dimen/_8font_mdp"
            app:layout_constraintBottom_toTopOf="@+id/simcard_security_user_input"
            app:layout_constraintStart_toStartOf="@+id/simcard_security_user_input"
            app:layout_constraintTop_toBottomOf="@+id/textView65" />

        <EditText
            android:id="@+id/simcard_security_user_input"
            android:layout_width="0dp"
            android:layout_height="40dp"
            android:layout_marginLeft="@dimen/_9mdp"
            android:layout_marginRight="@dimen/_9mdp"
            android:background="@drawable/back_btn_color"
            android:textSize="@dimen/_10font_mdp"
            android:ems="15"

            android:inputType="textPersonName"
            android:paddingLeft="5pt"
            app:layout_constraintBottom_toTopOf="@+id/textView77"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView5" />


        <TextView
            android:id="@+id/textView77"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_5mdp"


            android:text="Password"
            android:textColor="@color/white"
            android:textSize="@dimen/_8mdp"
            app:layout_constraintBottom_toTopOf="@+id/simcard_security_password_input"
            app:layout_constraintStart_toStartOf="@+id/simcard_security_password_input"
            app:layout_constraintTop_toBottomOf="@+id/simcard_security_user_input" />

        <EditText
            android:id="@+id/simcard_security_password_input"
            android:layout_width="0dp"
            android:layout_height="40dp"
            android:layout_marginBottom="134dp"
            android:background="@drawable/back_btn_color"
            android:ems="15"
            android:textSize="@dimen/_10font_mdp"
            android:inputType="textPersonName"
            android:paddingLeft="5pt"
            app:layout_constraintBottom_toTopOf="@+id/simcard_security_cancel"
            app:layout_constraintEnd_toEndOf="@+id/simcard_security_user_input"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="@+id/simcard_security_user_input"
            app:layout_constraintTop_toBottomOf="@+id/textView77" />


        <Button
            android:id="@+id/simcard_security_cancel"
            android:layout_width="@dimen/_40mdp"
            android:layout_height="@dimen/_15mdp"
            android:background="@drawable/back_btn_color"
            android:textColor="@color/white"
            android:textSize="@dimen/_7mdp"
            android:text="Cancel"

            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/simcard_security_ok"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/simcard_security_password_input" />

        <Button
            android:id="@+id/simcard_security_ok"
            android:layout_width="@dimen/_40mdp"
            android:layout_height="@dimen/_15mdp"
            android:background="@drawable/back_btn_color"
            android:textColor="@color/white"
            android:textSize="@dimen/_7mdp"
            android:text="OK"

            app:layout_constraintBottom_toBottomOf="@+id/simcard_security_cancel"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/simcard_security_cancel"
            app:layout_constraintTop_toTopOf="@+id/simcard_security_cancel" />


    </androidx.constraintlayout.widget.ConstraintLayout>



</FrameLayout>